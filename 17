package main

import "fmt"

func main() {
	var s string
	fmt.Println("Diga uma string (sem espa√ßos): ")
	fmt.Scan(&s)

	frequency := make(map[rune]int)
	for _, c := range s {
		frequencia[c]++
	}

	fmt.Println("Caracteres que aparecem somente uma vez:")
	for c, quantidade := range frequency {
		if quantidade == 1 {
			fmt.Printf("%c", c)
		}
	}
}
