package main

import (
	"fmt"
	"strings"
)

func main() {

	var str string

	fmt.Print("Digite uma string: ")
	fmt.Scanln(&str)

	temnum := false

	for _, characters := range str {

		if strings.ContainsAny(string(characters), "0123456789") {
			temnum = true
			break
		}
	}
	if temnum {
		fmt.Print("A string possui pelo menos um número!")
	} else {
		fmt.Print("A string não possui número!")
	}

}
